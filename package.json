    "menus": {
      "view/title": [
        {
          "command": "projectPanel.toggleConfigsGroupMode",
          "when": "view == projectPanelConfigs && !configsGrouped",
          "group": "navigation@1"
        },
        {
          "command": "projectPanel.toggleConfigsGroupModeGrouped",
          "when": "view == projectPanelConfigs && configsGrouped",
          "group": "navigation@1"
        },
        {
          "command": "projectPanel.openVariablesKeybindings",
          "when": "view == projectPanelConfigs",
          "group": "navigation@3"
        },
        {
          "command": "projectPanel.openVariablesConfig",
          "when": "view == projectPanelConfigs",
          "group": "navigation@2"
        },
        {
          "command": "projectPanel.toggleReplacementsGroupMode",
          "when": "view == projectPanelReplacements && !replacementsGrouped",
          "group": "navigation@1"
        },
        {
          "command": "projectPanel.toggleReplacementsGroupModeGrouped",
