          "group": "navigation@5"
        },
        {
          "command": "projectPanel.toggleTasksShowHiddenActive",
          "when": "view == projectPanelTasks && tasksHasHidden && tasksShowHidden",
          "group": "navigation@5"
        },
        {
          "command": "projectPanel.openTasksKeybindings",
          "when": "view == projectPanelTasks && taskSourcePP",
          "group": "navigation@6"
        },
        {
          "command": "projectPanel.toggleToolsGroupMode",
          "when": "view == projectPanelTools && !toolsGrouped",
          "group": "navigation@1"
        },
        {
          "command": "projectPanel.toggleToolsGroupModeGrouped",
          "when": "view == projectPanelTools && toolsGrouped",
          "group": "navigation@1"
        },
        {
          "command": "projectPanel.toggleToolsShowOnlyFavorites",
          "when": "view == projectPanelTools && toolsHasFavorites && !toolsShowOnlyFavorites",
          "group": "navigation@2"
        },
        {
          "command": "projectPanel.toggleToolsShowOnlyFavoritesActive",
          "when": "view == projectPanelTools && toolsHasFavorites && toolsShowOnlyFavorites",
          "group": "navigation@2"
        },
        {
          "command": "projectPanel.toggleToolsShowHidden",
          "when": "view == projectPanelTools && toolsHasHidden && !toolsShowHidden",
          "group": "navigation@3"
        },
        {
          "command": "projectPanel.toggleToolsShowHiddenActive",
          "when": "view == projectPanelTools && toolsHasHidden && toolsShowHidden",
          "group": "navigation@3"
        },
        {
          "command": "projectPanel.generateToolsDocs",
          "when": "view == projectPanelTools",
          "group": "navigation@4"
        },
        {
          "command": "projectPanel.addTool",
          "when": "view == projectPanelTools",
          "group": "navigation@5"
        },
        {
          "command": "projectPanel.togglePromptsGroupMode",
          "when": "view == projectPanelPrompts && !promptsGrouped",
