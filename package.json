          "group": "navigation@3"
        },
        {
          "command": "projectPanel.togglePromptsShowHiddenActive",
          "when": "view == projectPanelPrompts && promptsHasHidden && promptsShowHidden",
          "group": "navigation@3"
        },
        {
          "command": "projectPanel.addPrompt",
          "when": "view == projectPanelPrompts",
          "group": "navigation@4"
