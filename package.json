        {
          "command": "projectPanel.switchTaskSourceFromPP",
          "when": "view == projectPanelTasks && taskSourcePP",
          "group": "navigation@2"
        },
        {
          "command": "projectPanel.openTasksConfig",
          "when": "view == projectPanelTasks",
          "group": "navigation@3"
        },
        {
          "command": "projectPanel.toggleTasksShowOnlyFavorites",
