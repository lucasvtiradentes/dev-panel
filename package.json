          "when": "view == projectPanelTasks && !tasksGrouped && tasksHasGroups",
          "group": "navigation@1"
        },
        {
          "command": "projectPanel.toggleGroupModeGrouped",
          "when": "view == projectPanelTasks && tasksGrouped && tasksHasGroups",
