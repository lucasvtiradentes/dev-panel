{
  "name": "project-panel",
  "displayName": "Project Panel",
  "description": "VS Code extension to view and run tasks from Explorer pane",
  "icon": "resources/icon.png",
  "version": "0.0.1",
  "publisher": "lucasvtiradentes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasvtiradentes/project-panel"
  },
  "bugs": {
    "url": "https://github.com/lucasvtiradentes/project-panel/issues"
  },
  "homepage": "https://github.com/lucasvtiradentes/project-panel#readme",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "tasks",
    "task-runner",
    "project-tools"
  ],
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "main": "./out/extension.js",
  "scripts": {
    "prepare": "husky",
    "build": "tsx esbuild.config.ts",
    "postbuild": "tsx scripts/generate-schema.ts && tsx scripts/install-local/install-local-vscode.ts",
    "lint": "biome check src",
    "lint:fix": "biome check --fix --unsafe src",
    "format": "biome format --write src",
    "typecheck": "tsc --noEmit",
    "changeset": "changeset",
    "vscode:prepublish": "pnpm run build",
    "vscode:package": "vsce package --no-dependencies",
    "vscode:publish": "vsce publish --no-dependencies",
    "clean": "rimraf out dist-dev node_modules",
    "knip": "knip",
    "schema": "tsx scripts/generate-schema.ts"
  },
  "dependencies": {
    "json5": "^2.2.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.29.7",
    "@types/node": "^22.0.0",
    "@types/vscode": "^1.93.0",
    "esbuild": "^0.24.0",
    "husky": "^9.1.7",
    "knip": "^5.71.0",
    "lint-staged": "^15.2.11",
    "rimraf": "^6.0.1",
    "tscanner": "^0.1.3",
    "tsx": "^4.20.6",
    "typescript": "^5.7.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "projectPanel",
          "title": "Project Panel",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "projectPanel": [
        {
          "id": "projectPanelConfigs",
          "name": "Variables"
        },
        {
          "id": "projectPanelReplacements",
          "name": "Replacements"
        },
        {
          "id": "projectPanelTools",
          "name": "Tools"
        },
        {
          "id": "projectPanelPrompts",
          "name": "Prompts"
        },
        {
          "id": "projectPanelBranchContext",
          "name": "Branch Context"
        },
        {
          "id": "projectPanelTodos",
          "name": "Branch Tasks"
        }
      ],
      "explorer": [
        {
          "id": "projectPanelTasks",
          "name": "Task Runner",
          "icon": "$(tasklist)"
        }
      ]
    },
