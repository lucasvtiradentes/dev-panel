          "when": "view == projectPanelTodos",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "projectPanel.toggleFavorite",
          "when": "view == projectPanelTasks && (viewItem == task || viewItem == task-pp || viewItem == task-pp-global || viewItem == task-hidden || viewItem == task-pp-hidden || viewItem == task-pp-global-hidden)"
        },
        {
          "command": "projectPanel.toggleUnfavorite",
          "when": "view == projectPanelTasks && (viewItem == task-favorite || viewItem == task-pp-favorite || viewItem == task-pp-global-favorite)"
        },
        {
          "command": "projectPanel.toggleHide",
          "when": "view == projectPanelTasks && viewItem =~ /^task/"
        },
        {
          "command": "projectPanel.setTaskKeybinding",
          "when": "view == projectPanelTasks && viewItem =~ /^task/ && taskSourcePP"
        },
        {
          "command": "projectPanel.executeTask",
          "when": "view == projectPanelTasks"
